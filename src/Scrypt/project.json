{
  "name": "Scrypt.NET",
  "title": "Scrypt.NET",
  "version": "1.1.0",
  "description": "A .NET implementation of scrypt algorithm.",
  "language": "en-US",
  "authors": [ "Vinicius Chiele" ],

  "packOptions": {
    "projectUrl": "https://github.com/viniciuschiele/scrypt",
    "tags": [ "scrypt", "password", "hash", "hashing", "crypto", "cryptographic", "cryptography" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/viniciuschiele/scrypt"
    }
  },

  "buildOptions": {
    "allowUnsafe": true
  },

  "frameworks": {
    "net20": { },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "COREFX" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027",
        "System.Security.Cryptography.Algorithms": "4.1.0-rc2-24027"
      }
    }
  }
}
